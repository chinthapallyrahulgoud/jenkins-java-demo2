pipeline {
    agent any

    tools {
        maven 'Maven3'
        jdk 'Java11'
    }

    stages {
        stage('Checkout Code') {
            steps {
                git branch: 'main', url: 'https://github.com/YOUR_USERNAME/YOUR_REPO.git'
            }
        }

        stage('Build Maven Project') {
            steps {
                sh 'mvn clean package'
            }
        }

        stage('Test Completed') {
            steps {
                echo 'Build SUCCESS âœ”'
            }
        }
    }
}
